{
  "name": "capstone-game",
  "version": "0.0.1",
  "description": "Pioneers of Mars is a four-player game of expansion",
  "main": "index.js",
  "scripts": {
    "test": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register app/**/*.test.js app/**/*.test.jsx",
    "test-watch": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register --watch app/**/*.test.js app/**/*.test.jsx",
    "build": "check-node-version --node '>= 6.7.0' && bin/setup && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && bin/setup && webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colleenpurcell12/capstoneGame.git"
  },
  "keywords": [
    "react",
    "redux",
    "firebase"
  ],
  "author": "Sharon Choe, Deborah Kwon, Samantha Lugar, Colleen Purcell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/colleenpurcell12/capstoneGame/issues"
  },
  "homepage": "https://github.com/colleenpurcell12/capstoneGame/#readme",
